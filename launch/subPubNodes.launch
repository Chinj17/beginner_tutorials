<launch>
  <arg
    name = "frequency"
    default = "10"
    />
  <node
    name = "publisher"
    pkg = "beginner_tutorials"
    type = "talker"
    output = "screen"
  />
  <node
    name = "subscriber"
    pkg = "beginner_tutorials"
    type = "listener"
    output = "screen"
    launch-prefix = "gnome-terminal --command"
   />
</launch>
